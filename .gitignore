# Create .gitignore
cat > .gitignore << 'EOF'
# Dependencies
node_modules/
.pnp
.pnp.js

# Testing
coverage/
.nyc_output/

# Production
build/
dist/

# Environment files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local
coinbase-api-key.json

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS
.DS_Store
*.swp

# IDEs
.vscode/
.idea/

# Hardhat
cache/
artifacts/

# Wallet seeds (NEVER commit these!)
*.seed
wallet-backups/
EOF